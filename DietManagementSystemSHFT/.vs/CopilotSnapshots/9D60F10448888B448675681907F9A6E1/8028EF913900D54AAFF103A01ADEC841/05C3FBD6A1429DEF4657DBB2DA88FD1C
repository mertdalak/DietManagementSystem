using FluentValidation;
using DietManagementSystemSHFT.API.CQRS.Queries.DietitanQueries;

namespace DietManagementSystemSHFT.CQRS.Validators
{
    public class GetDietitianByIdQueryValidator : AbstractValidator<GetDietitianByIdQuery>
    {
        public GetDietitianByIdQueryValidator()
        {
            RuleFor(x => x.Id)
                .NotEmpty().WithMessage("Dietitian ID is required");
        }
    }
}